{
  "name": "inline-toggle-menu",
  "description": "A JS plugin that is similar to a dropdown menu, except instead of revealing additional items below, it will slide the menu inline.",
  "authors": [
    "Brandon sherette <bsherette@gmail.com> (http://brandonsherette.com)"
  ],
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/brandonsherette/inline-toggle-menu.git"
  },
  "main": [
    "dist/click-menu.js",
    "dist/click-menu.css"
  ],
  "ignore": [
    "/.*",
    "/*.js",
    "/*.md",
    "/api-docs",
    "/gulp*",
    "/package.json",
    "/src"
  ],
  "license": "MIT",
  "devDependencies": {
    "sinon": "http://sinonjs.org/releases/sinon-1.12.1.js"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "jquery": "~2.1.4",
    "font-awesome": "^4.5.0"
  },
  "keywords": [
    "javascript",
    "plugin",
    "nav",
    "click",
    "menu",
    "toggle",
    "inline-toggle-menu",
    "Inline Toggle Menu"
  ],
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/css/bootstrap.css",
        "dist/js/bootstrap.js"
      ]
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.css"
      ]
    }
  }
}
