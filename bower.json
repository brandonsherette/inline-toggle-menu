{
  "name": "click-menu",
  "description": "JavaScript click plugin that will slide into the nav link with a button press.",
  "authors": [
    "Brandon sherette"
  ],
  "version": "0.0.7",
  "main": [
    "dist/click-menu.js",
    "dist/click-menu.css"
  ],
  "ignore": [
    "/.*",
    "/*.js",
    "/*.md",
    "/api-docs",
    "/gulp*",
    "/package.json",
    "/src"
  ],
  "license": "MIT",
  "devDependencies": {
    "sinon": "http://sinonjs.org/releases/sinon-1.12.1.js"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "jquery": "~2.1.4",
    "font-awesome": "^4.5.0"
  },
  "keywords": [
    "JavaScript",
    "Plugin",
    "Click",
    "Menu",
    "Toggle"
  ],
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/css/bootstrap.css",
        "dist/js/bootstrap.js"
      ]
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.css"
      ]
    }
  }
}
